{
"languageserver": {
	"ccls":{
		"command": "ccls",
		"filetypes": ["c", "cpp"],
		"rootPatterns": ["compile_commands.json"],
		"initializationOptions": {
			"cache": {
				"directory": "/tmp/ccls-cache"
			},
			"highlight": {"lsRanges": true}
		}
	}
},
"diagnostic.enableSign": true,
"diagnostic.warningSign": "",
"signature.preferShownAbove": false,
"translator.engines":[
	"youdao"
]
}
